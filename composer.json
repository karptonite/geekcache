{
    "name": "geekcache/geekcache",
    "type": "library",
    "keywords": ["cache", "caching"],
    "homepage": "https://github.com/karptonite/geekcache",
    "description": "A wrapper for Memcached with tags, soft invalidation, memoization, and regeneration through callbacks",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Karp",
            "email": "danielkarp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ext-memcached": "*",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "dms/phpunit-arraysubset-asserts": "^0.5.0",
        "illuminate/container": "^12.5"
    },
    "autoload" :{
        "psr-4": {
            "GeekCache\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [ "tests" ]
    }
}
